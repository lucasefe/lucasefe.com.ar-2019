body{font-family:Cambria,Cochin,Georgia,Times,times new roman,serif;font-family:lucida sans,lucida sans regular,lucida grande,lucida sans unicode,Geneva,Verdana,sans-serif}a:before,a:after{content:none}p a{text-decoration:none;color:#000}.content .container.list ul li{display:flex;margin-top:20px;align-items:top}.content .container.list ul li .left{padding-right:20px}.content .container.list ul li .left img{border:solid 1px #000;max-width:80px}.content .container.list ul li .right{width:80%}.content .container.list ul li .right a{display:block;text-decoration:none}.content .container.list ul li .right span{margin:0;width:100%;text-align:left;font-size:80%;font-weight:700}.content .container.list ul li .right .precios{display:flex;flex-direction:row;justify-content:flex-start}.content .container.list ul li .right .precios span{flex-shrink:1;text-align:center;padding:0 10px;width:auto;margin-right:10px}.content .precios span{font-size:12px;padding:3px 8px;border-radius:5px}.content .precios span.aca{background-color:#18e022}.content .precios span.usa{background-color:#bdc6df}.content .precios span.meli{background-color:#fffb00}